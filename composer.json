{
    "name": "tedivm/stash-bundle",
    "type": "symfony-bundle",
    "description": "Incorporates the Stash caching library into Symfony.",
    "keywords": [
        "apc",
        "cache",
        "caching",
        "memcached",
        "redis",
        "sessions",
        "stash",
        "symfony"
    ],
    "homepage": "http://github.com/tedious/TedivmStashBundle",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Josh Hall-Bachner",
            "email": "jhallbachner@gmail.com"
        },
        {
            "name": "Robert Hafner",
            "email": "tedivm@tedivm.com"
        }
    ],
    "require": {
        "php": "^5.4|^7.0",
        "tedivm/stash": "0.14.*",
        "symfony/config": "~2.1|~3.0",
        "symfony/http-kernel": "~2.1|~3.0",
        "symfony/dependency-injection": "~2.1|~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*",
        "fabpot/php-cs-fixer": "0.4.0",
        "satooshi/php-coveralls": "1.0.*",
        "doctrine/cache": "~1.0"
    },
    "autoload": {
        "psr-4": {"Tedivm\\StashBundle\\": ""}
    }
}
